{
    "extends": "astro/tsconfigs/strict",
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist"
    ],
    "compilerOptions": {
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@content/*": ["./src/content/*"],
            "@pages/*": ["./src/pages/*"],
            "@styles/*": ["./src/styles/*"],
            "@utils/*": ["./src/utils/*"],
        }
    }
}